<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="VasarMAUI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:VasarMAUI"
    Shell.FlyoutBehavior="Disabled">

    <TabBar>
        <ShellContent
        Title="Ez a hét"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

        <ShellContent
        Title="Közelgő"
        ContentTemplate="{DataTemplate local:NextEvents}"
        Route="NextEvents" />
    </TabBar>

</Shell>
